<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
	
<mapper namespace="com.youxianji.dao.IProdTagInfoDAO" >
   
   <select id="getList" parameterType="java.lang.String" resultType="com.youxianji.pojo.ProdTagInfo">
         select  
            *
            from prodtaginfo where state='1' and tagid!='1611262243180000' order by serialnumber asc,createTime desc
   </select>
  	
  	
  	

</mapper>